{
  "manifest_version": 2,
  "name": "Tweak New Twitter",
  "description": "Reduce \"engagement\" and tone down some of New Twitter's UI",
  "homepage_url": "https://github.com/insin/tweak-new-twitter/",
  "version": "1.8",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "./tweak-new-twitter.user.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ]
}
