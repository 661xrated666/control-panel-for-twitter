{
  "manifest_version": 2,
  "name": "Tweak New Twitter",
  "description": "Always use Latest Tweets, manage retweets, and other UI tweaks for New Twitter",
  "homepage_url": "https://github.com/insin/tweak-new-twitter/",
  "version": "1.4",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": ["./tweak-new-twitter.user.js"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage"]
}
